<h1>Listing baselines</h1>

<table>
  <tr>
    <th>Election</th>
    <th>Voter</th>
    <th>Current</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @baselines.each do |baseline| %>
  <tr>
    <td><%= baseline.election %></td>
    <td><%= baseline.voter %></td>
    <td><%= baseline.current %></td>
    <td><%= link_to 'Show', baseline %></td>
    <td><%= link_to 'Edit', edit_baseline_path(baseline) %></td>
    <td><%= link_to 'Destroy', baseline, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

